package com.irtve.portal.core2.page;

import java.util.List;

/**
 * Inmutable page descriptor bean.
 * 
 * @author ernesto
 * 
 */
public class PageDescriptor {

	public final String view;
	public final String template;
	public final String skin;
	public final List<String> modules;
	public final String errorPolicy;
	
	/**
	 * 
	 * @param view
	 * @param template
	 * @param skin
	 * @param modules
	 * @param errorPolicy
	 */
	public PageDescriptor(String view, String template, String skin, List<String> modules, String errorPolicy) {
		this.view = view;
		this.template = template;
		this.skin = skin;
		this.modules = modules;
		this.errorPolicy = errorPolicy;
	}
	
}
