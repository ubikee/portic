<%@ tag body-content="empty" pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ attribute name="data" required="true" type="com.irtve.portal.core2.Model"%>

<div class="header">

<div class="image1"></div>

<div class="title">${data.title}</div>

<div class="label">${data.label}</div>

<c:if test="${not empty data.menu1}">
	<div class="menu1">${data.menu1}</div>
</c:if>

<c:if test="${not empty data.menu2}">
	<div class="menu2">${data.menu2}</div>
</c:if>

</div>